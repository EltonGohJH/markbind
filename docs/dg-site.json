{
  "baseUrl": "/devdocs",
  "titlePrefix": "MarkBind",
  "style": {
    "codeTheme": "light"
  },
  "pages": [
    {
      "glob": ["*.md", "devGuide/*.md", "devGuide/*/*.md"]
    },
    {
      "src": ["index.md", "devGuide/index.md"],
      "searchable": "no"
    }
  ],
  "headingIndexingLevel": 6,
  "ignore": [
    "_markbind",
    "_site/*",
    "lib/*",
    "*.json",
    "*.md",
    "*.njk",
    ".git/*",
    "*.pptx",
    "CNAME"
  ],
  "plugins" : [
    "filterTags",
    "mathDelimiters",
    "codeBlockWrapButtons",
    "web3Form",
    "codeBlockCopyButtons",
    "dataTable",
    "mermaid"
  ],
  "pluginsContext" : {
    "filterTags" : {
      "tags": ["environment--dg"]
    },
    "mathDelimiters": {
      "delimiters": ["beg_end"]
    },
    "web3Form": {
      "accessKey": [""]
    }
  },
  "deploy": {
    "message": "Dev Site Update.",
    "repo": "https://github.com/MarkBind/devdocs.git",
    "branch": "gh-pages"
  }
}
